#set( $subject = "${subject}")

#parse("/velocity/templates/email/fragments/header.vm")
<div
    style="width: 1903px; color: rgb(51, 51, 51); font-family: &amp; quot; Helvetica Neue&amp;quot; , Helvetica ,Arial,sans-serif; font-size: 14px; line-height: 20px">
    <div
    style="width: 1903px; color: rgb(51, 51, 51); font-family: &amp; quot; Helvetica Neue&amp;quot; , Helvetica ,Arial,sans-serif; font-size: 14px; line-height: 20px">
    <div style="">
        <span
            style="padding-left: 13px; font-family: &amp; quot; times new roman&amp;quot;; font-size: 2em">Delivery
            Interface</span>
    </div>
    <div style="min-height: 896px; margin: 0px">
        <div style="padding-top: 15px">
            <div>
                <div
                    style="min-height: 1px; padding-right: 15px; padding-left: 15px; float: left; width: 100%">
                    <div
                        style="padding: 15px 35px 15px 15px; margin-bottom: 20px; border: 1px solid rgb(235, 204, 209); border-radius: 4px; color: rgb(169, 68, 66); background-color: rgb(242, 222, 222)">
                        <h4
                            style="font-family: inherit; font-weight: 500; line-height: 1.1; color: inherit; margin-top: 0px; margin-bottom: 10px; font-size: 18px">Alert!</h4>
                        <span style="font-weight: 700"></span>&nbsp;
                        $!errorMessage
                    </div>
                </div>
            </div>
            <div
                style="min-height: 20px; padding: 19px; margin-bottom: 20px; border: 1px solid rgb(227, 227, 227); border-radius: 4px; background-color: rgb(245, 245, 245)">
                <h4
                    style="font-family: inherit; font-weight: 500; line-height: 1.1; color: inherit; margin-top: 10px; margin-bottom: 10px; font-size: 18px">
                    Order&nbsp;<span
                        style="color: black; font-weight: bold; font-size: 1.2em; margin-right: 20px">${orderId}</span> <a href="${protocol}://${hostName}:${portNumber}/DeliveryInterface/dashboard/order/${orderId}">View Details</a> &nbsp;&nbsp;Club Id :<span
                        style="color: black; font-weight: bold; font-size: 1.2em; margin-right: 20px">$!clubId</span>
                </h4>
                <ul style="margin-top: 0px; margin-bottom: 20px; padding-left: 0px">
                    <li
                        style="display: block; padding: 10px 15px; border: 1px solid rgb(221, 221, 221); border-top-left-radius: 4px; border-top-right-radius: 4px; background-color: rgb(255, 255, 255)"><span>Date
                            created:&nbsp;</span><span
                        style="display: inline; padding: 0.2em 0.6em 0.3em; font-size: 10.5px; font-weight: 700; line-height: 1; color: rgb(255, 255, 255); text-align: center; white-space: nowrap; vertical-align: baseline; border-radius: 0.25em; background-color: rgb(92, 184, 92)">$!createdDate</span></li>
                    <li
                        style="display: block; padding: 10px 15px; border: 1px solid rgb(221, 221, 221); background-color: rgb(255, 255, 255)"><span>Last
                            update:</span>&nbsp;<span
                        style="display: inline; padding: 0.2em 0.6em 0.3em; font-size: 10.5px; font-weight: 700; line-height: 1; color: rgb(255, 255, 255); text-align: center; white-space: nowrap; vertical-align: baseline; border-radius: 0.25em; background-color: rgb(92, 184, 92)">$!lastModifiedDate</span></li>
                    <li
                        style="display: block; padding: 10px 15px; border: 1px solid rgb(221, 221, 221); background-color: rgb(255, 255, 255)"><span>Vendor
                            Name:</span>&nbsp;<span
                        style="display: inline; padding: 0.2em 0.6em 0.3em; font-size: 10.5px; font-weight: 700; line-height: 1; color: rgb(255, 255, 255); text-align: center; white-space: nowrap; vertical-align: baseline; border-radius: 0.25em; background-color: rgb(92, 184, 92)">$!vendorName</span></li>
                </ul>
               <span>This is an automated notification please do not reply.</span>
            </div>
        </div>
    </div>
</div>
#parse("/velocity/templates/email/fragments/footer.vm")
